<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.autovibe.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home"
        />

    <fragment
        android:id="@+id/navigation_car_info"
        android:name="com.autovibe.ui.carInfo.CarInfoFragment"
        android:label="@string/title_car_info"
        tools:layout="@layout/fragment_car_info" />

    <fragment
        android:id="@+id/navigation_contact_us"
        android:name="com.autovibe.ui.contactUs.ContactUsFragment"
        android:label="@string/title_contact_us"
        tools:layout="@layout/fragment_contact_us" />
</navigation>